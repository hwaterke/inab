{
  "name": "inab-server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node src/index.js",
    "test": "jest --runInBand",
    "prebuild": "rm -r build || true",
    "build": "babel src --copy-files --out-dir build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "boom": "^7.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-promise-router": "^3.0.2",
    "helmet": "^3.12.0",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "sqlite3": "^4.0.0",
    "typeorm": "^0.2.0-alpha.46"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.4.4",
    "lint-staged": "^7.1.0",
    "nodemon": "^1.17.4",
    "supertest": "^3.1.0"
  },
  "lint-staged": {
    "*.js": ["eslint --max-warnings 0", "git add"]
  },
  "jest": {
    "testMatch": ["**/?(*.)(spec|test).js?(x)"],
    "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"]
  }
}
