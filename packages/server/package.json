{
  "name": "@inab/server",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node src/index.js",
    "test": "jest --runInBand",
    "prebuild": "rm -r build || true",
    "build": "babel src --copy-files --out-dir build"
  },
  "dependencies": {
    "bcrypt": "3.0.0",
    "boom": "7.2.0",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-promise-router": "3.0.3",
    "helmet": "3.13.0",
    "joi": "13.6.0",
    "jsonwebtoken": "8.3.0",
    "morgan": "1.9.0",
    "ramda": "0.25.0",
    "sqlite3": "4.0.2",
    "typeorm": "0.2.7"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-rc.3",
    "@babel/core": "7.0.0-rc.3",
    "@babel/node": "7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.3",
    "@babel/plugin-proposal-decorators": "7.0.0-rc.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.3",
    "@babel/plugin-syntax-import-meta": "^7.0.0-rc.3",
    "@babel/preset-env": "7.0.0-rc.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "eslint": "5.4.0",
    "eslint-plugin-import": "2.14.0",
    "jest": "23.5.0",
    "lint-staged": "8.1.4",
    "nodemon": "1.18.3",
    "supertest": "3.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings 0",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ]
  }
}
